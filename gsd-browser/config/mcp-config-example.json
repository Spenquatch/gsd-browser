{
  "mcpServers": {
    "gsd": {
      "type": "stdio",
      "command": "gsd",
      "args": ["mcp", "serve"],
      "env": {
        "GSD_LLM_PROVIDER": "anthropic",
        "GSD_MODEL": "claude-haiku-4-5",
        "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}"
      },
      "description": "GSD MCP server"
    }
  }
}
