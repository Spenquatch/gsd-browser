# Copy to `.env` and fill in the provider you plan to use.

# LLM provider: anthropic | openai | chatbrowseruse | ollama
GSD_BROWSER_LLM_PROVIDER=anthropic

# Model name is provider-specific.
GSD_BROWSER_MODEL=claude-haiku-4-5

# --- Cloud provider API keys ---
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# browser-use cloud API key (ChatBrowserUse); optional URL override.
BROWSER_USE_API_KEY=
BROWSER_USE_LLM_URL=

# --- Local OSS path (Ollama) ---
OLLAMA_HOST=http://localhost:11434

# --- Logging ---
LOG_LEVEL=INFO
GSD_BROWSER_JSON_LOGS=false

# --- Streaming (optional) ---
STREAMING_MODE=cdp
STREAMING_QUALITY=med

