{
  "name": "gsd-browser-ts",
  "version": "0.1.0",
  "description": "Reusable MCP server template implemented in TypeScript",
  "type": "module",
  "bin": {
    "gsd-browser-ts": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/cli.ts serve",
    "lint": "eslint .",
    "test": "vitest run",
    "smoke": "npm run build && node dist/cli.js serve --once",
    "diagnose": "node dist/cli.js diagnose"
  },
  "keywords": ["mcp", "template", "typescript"],
  "author": "InboxGreen",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "toml": "^3.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@types/node": "^22.10.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.4"
  }
}
